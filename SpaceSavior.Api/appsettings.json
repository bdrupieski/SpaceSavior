{
  "RateConfigurationSettings": {
    "DefaultRatesInputFilePath": "rates.json"
  },
  "AspNetMetrics": {
    "DefaultContextLabel": "SpaceSavior",
    "HealthEndpointEnabled": true,
    "MetricsEndpointEnabled": true,
    "PingEndpointEnabled": false,
    "MetricsTextEndpointEnabled": false,
    "OAuth2TrackingEnabled": false,
    "ApdexTrackingEnabled": false,
    "HealthEndpoint": "/appmetrics/health",
    "MetricsEndpoint": "/appmetrics/metrics",
    "EnvironmentInfoEndpoint": "/appmetrics/environment"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  }
}
